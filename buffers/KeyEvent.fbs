namespace Disseminate.Key;

enum Type : byte { Up, Down }

struct Location {
    x: float;
    y: float;
}

table Event
{
    type: Type;
    keyCode: int;
    location: Location;
    windowNumber: int;
    modifiers: ushort = 0;
    timestamp: double = 0;
    repeat: bool = false;
    text: string;
    fromUuid: string;
}

root_type Event;
